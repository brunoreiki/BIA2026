/* image = légende */

    .gallery {
      display: flex;
      flex-direction: column;
      justify-content: flex-start;
      align-items: center;
      gap: 20px;
      padding: 20px;
      background-color: #00274d; /* Bleu foncé */
      color: white;
    }



    .gallery-titre {
      text-align: center;
      color: white;
      font-weight: bold;
      margin: 0;
      font-size: 2em;
    }

    .gallery-items {
      display: flex;
      justify-content: space-around;
      align-items: flex-end;
      gap: 20px;
      flex-wrap: wrap;
      width: 100%;
    }

    .gallery-item {
      text-align: center;
      max-width: 250px;
    }

    .gallery-item img {
      max-width: 100%;
      height: auto;
      border: 2px solid white;
      border-radius: 0; /* Coins carrés */
    }

    .gallery-item figcaption {
      color: white; /* Légendes en blanc */
      font-style: italic;
      margin-top: 10px;
    }

/* epicea */
        .bloc-image-text {
            font-family: Arial, sans-serif;
            line-height: 1.6;
            margin: 0;
            padding: 0;
            display: flex;
            flex-wrap: wrap;
            gap: 20px;
            justify-content: center;
            color: white;
        }
        .column {
            flex: 1 1 calc(50% - 40px);
            box-sizing: border-box;
            padding: 20px;
            border-radius: 8px;
            background-color: #004488;
        }
        .entry {
            display: grid;
            grid-template-columns: auto 1fr;
            grid-template-rows: auto auto;
            align-items: start;
            gap: 15px;
            margin-bottom: 20px;
        }
        .entry img {
            grid-row: 1 / span 2;
            grid-column: 1;
            flex-shrink: 0;
        }
        .entry .text {
            grid-column: 2;
            grid-row: 1;
        }
        .entry .legend {
            grid-column: 2;
            grid-row: 2;
            font-style: italic;
            margin-top: 10px;
            color: #cce6ff;
        }
        .titre-bloc-image {
            margin-top: 0;
            color: #ffffff;
            font-size: 1.5em;
            font-weight: bold;
        }

        .text p {
            margin-top: 0;
            color: #ffffff;
            font-weight: bold;
            margin: 0 0 10px;
        }

