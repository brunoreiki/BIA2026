  <question type="multichoice">
    <name>
      <text><![CDATA[Un avion en virage subit 2 g. Quel est son angle d’inclinaison:]]></text>
    </name>
    <questiontext format="html">
      <text><![CDATA[Un avion en virage subit 2 g. Quel est son angle d’inclinaison:]]></text>
    </questiontext>
