    <name>
      <text><![CDATA[Pour réduire la traînée induite d'une aile, on peut:]]></text>
    </name>
    <questiontext format="html">
      <text><![CDATA[Pour réduire la traînée induite d'une aile, on peut:]]></text>
    </questiontext>
    <answer fraction="100" format="html">
      <text><![CDATA[augmenter l'allongement de l'aile]]></text>
      <feedback>
        <text/>
      </feedback>
