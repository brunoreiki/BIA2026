  <question type="multichoice">
    <name>
      <text>Le vol VFR est:</text>
    </name>
    <questiontext format="html">
      <text><![CDATA[Le vol VFR est:]]></text>
    </questiontext>
    <answer fraction="100" format="html">
      <text><![CDATA[un vol effectué dans des conditions de vol à vue]]></text>
      <feedback>
        <text/>
